                "service": "read",
                "to": "read"
              },
              {
                "id": "delete",
                "label": "Delete",
                "service": "delete_data",
                "to": "delete"
              },
              {
                "id": "query",
                "label": "Query",
                "service": "query",
                "to": "query"
              }
            ],
